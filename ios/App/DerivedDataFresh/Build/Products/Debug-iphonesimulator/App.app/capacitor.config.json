{
	"appId": "com.idoneo.app",
	"appName": "Idoneo",
	"webDir": "dist",
	"server": {
		"iosScheme": "https"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 2000,
			"launchAutoHide": true,
			"backgroundColor": "#00B1FF",
			"showSpinner": false,
			"iosSpinnerStyle": "small",
			"spinnerColor": "#ffffff",
			"splashFullScreen": true,
			"splashImmersive": true
		},
		"StatusBar": {
			"style": "LIGHT",
			"backgroundColor": "#00B1FF"
		},
		"Keyboard": {
			"resizeOnFullScreen": true
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"ios": {
		"contentInset": "automatic",
		"preferredContentMode": "mobile",
		"allowsLinkPreview": false,
		"scheme": "idoneo"
	},
	"packageClassList": [
		"AppPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"PushNotificationsPlugin",
		"SharePlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin"
	]
}
